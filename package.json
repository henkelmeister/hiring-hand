    "@babel/eslint-parser": "^7.15.7",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-react": "^7.26.0",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
