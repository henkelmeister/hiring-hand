    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "webpack-dev-server": "^4.2.1"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--experimental-specifier-resolution=node webpack-cli",
    "build:serve": "NODE_OPTIONS=--experimental-specifier-resolution=node webpack-cli serve",
    "clean": "run-s clean:build clean:log clean:yarn",
    "clean:build": "rimraf 'public'",
    "clean:log": "rimraf 'log'",
    "clean:yarn": "rimraf 'yarn-error.log'",
    "db:create": "NODE_OPTIONS=--experimental-specifier-resolution=node node ./db/util/create",
    "db:drop": "NODE_OPTIONS=--experimental-specifier-resolution=node node ./db/util/drop",
    "db:migrate": "NODE_OPTIONS=--experimental-specifier-resolution=node knex migrate:latest",
    "db:rollback": "NODE_OPTIONS=--experimental-specifier-resolution=node knex migrate:rollback",
    "db:seed": "NODE_OPTIONS=--experimental-specifier-resolution=node knex seed:run",
    "lint": "run-s lint:js lint:style",
    "lint:js": "eslint . --ext js,jsx,mjs --fix",
    "lint:style": "stylelint **/*.{css,scss} --fix",
    "serve": "NODE_OPTIONS=--experimental-specifier-resolution=node node .",
    "dev": "run-p dev:build:serve dev:serve",
    "dev:build": "NODE_ENV=development yarn run build",
    "dev:build:serve": "NODE_ENV=development yarn run build:serve",
    "dev:db:create": "NODE_ENV=development yarn run db:create",
    "dev:db:drop": "NODE_ENV=development yarn run db:drop",
    "dev:db:migrate": "NODE_ENV=development yarn run db:migrate",
    "dev:db:rollback": "NODE_ENV=development yarn run db:rollback",
    "dev:db:seed": "NODE_ENV=development yarn run db:seed",
    "dev:serve": "NODE_ENV=development NODE_OPTIONS=--experimental-specifier-resolution=node nodemon .",
    "prod": "run-s prod:build prod:serve",
    "prod:build": "NODE_ENV=production yarn run build",
    "prod:build:serve": "NODE_ENV=production yarn run build:serve",
    "prod:db:create": "NODE_ENV=production yarn run db:create",
    "prod:db:drop": "NODE_ENV=production yarn run db:drop",
    "prod:db:migrate": "NODE_ENV=production yarn run db:migrate",
    "prod:db:rollback": "NODE_ENV=prouction yarn run db:rollback",
    "prod:db:seed": "NODE_ENV=production yarn run db:seed",
    "prod:serve": "NODE_ENV=production yarn run serve",
    "test": "run-s test:build test:serve",
    "test:build": "NODE_ENV=test yarn run build",
    "test:build:serve": "NODE_ENV=test yarn run build:serve",
    "test:db:create": "NODE_ENV=test yarn run db:create",
    "test:db:drop": "NODE_ENV=test yarn run db:drop",
    "test:db:migrate": "NODE_ENV=test yarn run db:migrate",
    "test:db:rollback": "NODE_ENV=test yarn run db:rollback",
    "test:db:seed": "NODE_ENV=test yarn run db:seed",
    "test:serve": "NODE_ENV=test yarn run serve"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
